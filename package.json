    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mirador": "^3.3.0",
    "nwb": "^0.25.2",
    "prettier": "^2.8.8",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
